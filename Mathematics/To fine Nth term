// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int N;
    cin>>N;
        int a=9;
        int b=33;
        
        if(N==1)
            return a;
        if(N==2)
            return b;
        int diff,c;
        for(int i=3; i<=N;i++){
            diff = b-a  ;        
            c = b + diff +16;       
            a = b;                  
            b = c;                  
        }
        cout<< c;
    return 0;
}
