// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int N;
    cin>>N;
        int sum = 0;
        while(N>9){
            while(N!=0){
            sum = sum + N%10;
            N=N/10;
            }
            //sum = 9, N=0
            N=sum;
            sum=0;
            //sum = 0, N=9
        }
        
        cout<<N;
    return 0;
}
