// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
    // array<int,5>arr={1,2,3,4,5};
    int N;
    cin>>N;
    int sum = 0;
        while(N>9){
            while(N){
            sum = sum + N%10;
            N=N/10;
            }
            N=sum;
            sum=0;
        }
        
        cout<< N;
}
