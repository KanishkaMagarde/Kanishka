#include <iostream>
using namespace std;
int main() {
    int t1=0,t2=1,tn=0;
    int n;
    cin>>n;
        for (int i=1; i<=n; i++){
            if(i==1){
                cout<<t1;
            }
            if(i==2){
                cout<<t2;
            }
            tn=t1+t2;
            t1=t2;
            t2=tn;
            cout<< tn;
        }
        
    return 0;
}
