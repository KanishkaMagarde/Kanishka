#include <iostream>
using namespace std;

int main() {
    int num;
    cout<<"Enter number"<<endl;
    cin>>num;
    int sum=0;
    while(num!=0){
        sum=sum+(num%10);
        num=num/10;
    }
    if (sum%5==0){
        cout<<"Sum of number is divisible by 5";
    }
    else{
        cout<<"Sum of number is not divisible by 5";
    }
    return 0;
}
