#include <iostream>
using namespace std;
#include <vector>

int main() {
    int n;
    
    cout<<"Enter number of elements : ";
    cin>>n;
    vector<int>A(n);
    for (int i=0;i<n;i++){
        cout<<"Enter element : ";
        cin>>A[i];
    }
    cout<<"Given Array Contains : ";
    for (int i=0;i<n;i++){
        cout<<A[i]<<","<<endl;
    }
    int sum=0;
    for (int i=0;i<n;i++){
        sum=sum+A[i];
    }
    int mean=sum/n;
    cout<<"Mean of the given array is : "<<mean;
    return 0;
}
