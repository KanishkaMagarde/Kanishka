#include <iostream>
using namespace std;

int main() {
    int num;
    cout<<"Enter number"<<endl;
    cin>>num;
    int number=0;
    int digit;
    //int place=1;
    while(num!=0){
        digit=num%10;
        if (digit==0){
            digit=5;
        }
        num=num/10;
        number=number*10+digit;
        //place=place*10;
    }
    int ans=0;
    while (number!=0){
        ans=ans*10+(number%10);
        number=number/10;
    }
    
    cout<<ans;
    return 0;
}
