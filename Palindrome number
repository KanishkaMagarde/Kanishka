#include <iostream>
using namespace std;

int main() {
    int num;
    cout<<"Enter number"<<endl;
    cin>>num;
    int org=num;
    int rev=0;
    while(num!=0){
        rev=rev*10+num%10;
        num=num/10;
    }
    if(org==rev){
        cout<<"It is a palindrome number";
    }
    else{
        cout<<"It is not a palindrome number";
    }


    return 0;
}
